{
  "name": "cloves-plural-discord-bot",
  "version": "1.0.0",
  "description": "A Discord bot for plural systems based on PluralKit",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
    "watch": "nodemon --exec ts-node src/main.ts",
    "deploy-commands": "ts-node scripts/deploy-commands.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "plural",
    "pluralkit",
    "typescript"
  ],
  "author": "Clove Twilight",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^1.7.1",
    "discord-api-types": "^0.37.56",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}